{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Main",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "internalConsoleOptions": "openOnSessionStart",
            "sourceMaps": true,
            "preLaunchTask": "npm: build_main:dev"
        }
    ]
}